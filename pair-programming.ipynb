    "import math\n",
    "def polar_to_cartesian(r, theta):\n",
    "    \"\"\"takes two values in polar co-ordinates and returns a tuple in cartesian co-ordinates\"\"\"\n",
    "    x = r*math.cos(theta)\n",
    "    y = r*math.sin(theta)\n",
    "    return x, y"
   ]
  },
